        </geometry>
        <material name="dark"/>
    </visual>
</link>

<link name="arm_${id}_link_6">
    <inertial>
        <mass value="0.1"/>
        <inertia ixx="0.0001" ixy="0" ixz="0" iyy="0.0001" iyz="0" izz="0.0001"/>
        <origin xyz="0 0 0"/>
    </inertial>
    <collision name="arm_${id}_link_6_collision">
